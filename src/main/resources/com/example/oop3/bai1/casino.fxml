<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="root" prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.oop3.bai1.CasinoController">

    <children>
        <Label layoutX="14.0" layoutY="14.0" text="Số thứ nhất " />
        <TextField fx:id="num1TextField" layoutX="14.0" layoutY="33.0" prefHeight="25.0" prefWidth="272.0" />
        <Label layoutX="14.0" layoutY="75.0" text="Số thứ hai " />
        <TextField fx:id="num2TextField" layoutX="14.0" layoutY="94.0" prefHeight="25.0" prefWidth="272.0" />
        <Button fx:id="addButton" layoutX="14.0" layoutY="137.0" mnemonicParsing="false" onAction="#add" text="+" />
        <Button fx:id="subtractButton" layoutX="78.0" layoutY="137.0" mnemonicParsing="false" onAction="#subtract" text="-" />
        <Button fx:id="multiplyButton" layoutX="142.0" layoutY="137.0" mnemonicParsing="false" onAction="#multiply" text="*" />
        <Button fx:id="divideButton" layoutX="206.0" layoutY="137.0" mnemonicParsing="false" onAction="#divide" text="/" />
    </children>
</AnchorPane>